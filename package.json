{
  "name": "jabos-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=e2e,performance,security",
    "test:api": "jest --testPathPattern=api",
    "test:database": "node tests/database-integration-tests.js",
    "test:security": "jest --testPathPattern=security",
    "test:performance": "jest --testPathPattern=performance --timeout=60000",
    "test:all": "npm run test:unit && npm run test:api && npm run test:database && npm run test:security && npm run test:performance",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests --maxWorkers=2",
    "test:watch": "jest --watch",
    "test:debug": "jest --detectOpenHandles --forceExit --verbose",
    "test:payment": "node tests/comprehensive-payment-system-tests.js",
    "test:security:comprehensive": "node scripts/run-comprehensive-security-tests.mjs",
    "test:security:production": "NODE_ENV=production node scripts/run-comprehensive-security-tests.mjs",
    "test:security:validate": "node scripts/validate-security-testing-setup.mjs",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase migration up",
    "types:generate": "bash scripts/generate-db-types.sh",
    "format": "prettier --write \"**/*.{js,ts,json,sql}\"",
    "depcruise": "depcruise --config .dependency-cruiser.js",
    "depcruise:validate": "depcruise --validate --config .dependency-cruiser.js .",
    "depcruise:html": "depcruise --config .dependency-cruiser.js --output-type html --output-to dependency-report.html .",
    "seed:demo-flow": "node scripts/seed-demo-match-flow.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.0",
    "astronomy-engine": "^2.1.19",
    "circular-natal-horoscope-js": "^1.1.0",
    "date-fns": "^2.30.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@types/uuid": "^9.0.2",
    "axios": "^1.11.0",
    "chalk": "^5.5.0",
    "dependency-cruiser": "^16.10.1",
    "dotenv": "^17.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.1",
    "stripe": "^18.0.0",
    "supabase": "^2.48.3",
    "typescript": "^5.1.6"
  }
}
